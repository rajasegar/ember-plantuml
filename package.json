{
  "name": "ember-plantuml",
  "version": "0.2.0",
  "description": "CLI to visualize your Ember.js Components and Models using usinPlantUML in the browser",
  "repository": "https://github.com/rajasegar/ember-plantuml",
  "license": "MIT",
  "author": "Rajasegar Chandran <rajasegar.c@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "bin": {
    "ember-plantuml": "./bin/ember-plantuml.js"
  },
  "keywords": [
    "ember",
    "plantuml",
    "ember.js",
    "uml",
    "architecture"
  ],
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "build:watch": "ember build --watch",
    "start:cli": "./bin/ember-plantuml.js",
    "dev": "concurrently \"npm run build:watch\" \"npm run start:cli\"",
    "deploy": "git push && git push --tags && npm publish",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.5.0",
    "@glimmer/component": "1.0.4",
    "@glimmer/tracking": "1.0.4",
    "@semantic-release/changelog": "6.0.0",
    "@semantic-release/git": "9.0.1",
    "babel-eslint": "10.1.0",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "6.3.0",
    "ember-auto-import": "1.12.0",
    "ember-cli": "3.28.3",
    "ember-cli-app-version": "5.0.0",
    "ember-cli-babel": "7.26.6",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-htmlbars": "5.7.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-export-application-global": "2.0.1",
    "ember-fetch": "8.1.1",
    "ember-load-initializers": "2.1.2",
    "ember-maybe-import-regenerator": "1.0.0",
    "ember-page-title": "6.2.2",
    "ember-qunit": "5.1.5",
    "ember-resolver": "8.0.3",
    "ember-source": "3.28.4",
    "ember-template-lint": "3.10.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-ember": "10.5.7",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "qunit": "2.17.2",
    "qunit-dom": "2.0.0",
    "semantic-release": "18.0.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "debug": "4.3.2",
    "ember-gen-uml": "2.1.13",
    "express": "4.17.1",
    "get-port": "6.0.0",
    "plantuml": "0.0.1",
    "walk-sync": "3.0.0"
  },
   "release": {
     "branches": ["main"],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "release(version): Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
